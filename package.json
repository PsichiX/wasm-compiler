{
  "name": "@intuicio/wasm-compiler",
  "version": "1.0.0",
  "description": "WASM compiler (supports compilation between formats: WAT, WASM, AST)",
  "main": "./bin/cli.js",
  "scripts": {
    "postinstall": "npm run build",
    "build-api": "./node_modules/.bin/babel src --out-dir bin",
    "build-browserify": "./node_modules/.bin/browserify bin/index -o bin/wasm-compiler.js -s WasmCompiler",
    "build-uglify": "./node_modules/.bin/uglifyjs -nm -o bin/wasm-compiler.min.js bin/wasm-compiler.js",
    "build-web": "npm run build-browserify",
    "build": "npm run build-api && npm run build-web",
    "clean": "rm -r bin"
  },
  "bin": {
    "wasmc": "./bin/cli.js"
  },
  "author": "Patryk 'PsichiX' Budzynski <psichix@gmail.com> (http://psichix.io)",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.46",
    "@babel/core": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.0.0-beta.46",
    "browserify": "^16.2.2",
    "uglify-js": "^3.3.24",
    "yargs": "^11.0.0"
  }
}
